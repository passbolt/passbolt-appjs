<li class="open node root group-item" id="{{ mappedItem.id }}">
    <div class="row ">
        <div class="main-cell-wrapper">
            <div class="main-cell">
                <a title="{{ mappedItem.label }}'"><span>{{ mappedItem.label }}</span></a>
            </div>
        </div>
        {{#if(withMenu)}}
        {{#if(mappedItem.canEdit)}}
        <div class="right-cell more-ctrl">
            <a><span>more</span></a>
        </div>
        {{/if}}
        {{/if}}
    </div>
</li>